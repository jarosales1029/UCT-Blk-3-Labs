def sum_up(numbers):
    num = []
    for i in str(numbers):
        num.append(int(i))
    sum_of_all = 0
    for j in num:
        sum_of_all += j
        
    return sum_of_all
    
print(sum_up(451))
print(sum_up(13843))
print(sum_up(10))


def sum_upped(numbers):
    sum_of_allz = 0
    for i in str(numbers):
        sum_of_allz += int(i)
    return sum_of_allz
    
print(sum_up(451))
print(sum_up(13843))
print(sum_up(10))
